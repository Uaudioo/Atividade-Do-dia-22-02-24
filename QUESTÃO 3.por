programa{

funcao inicio(){

inteiro a,b,c,soma

//Pedindo Número
escreva("Insira o Primeiro Número: ")
leia(a)

escreva("Insira o Segundo Número: ")
leia(b)

//calculo
se(a==b){
  c=a+b
} 
se(a!=b){
  c=a*b
}




//Exibindo
escreva("==============================================================")
escreva("\nResultado: ",c)



}





}